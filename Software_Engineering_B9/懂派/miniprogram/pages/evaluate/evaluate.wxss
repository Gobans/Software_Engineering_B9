/* pages/evaluate/evaluate.wxss */

.comment1-description{
    padding:30rpx 0;
    display:flex;
    flex-direction:row;
    align-self:center;
    margin-top: 5%;
    margin-bottom: 5%;
  }

  .evaluate-txt{
    align-self:center;
    font-size: 1.2em;
    margin-bottom: 5%;
  }

  .stars{
      background:url(data:image/png;base64,);
      width: 80rpx;
      height: 80rpx;
      background-size: 80rpx 80rpx;
      margin-left: 30rpx;
      transition: 600ms;
  }

  .on{
      background:url("data:image/png;base64,");
      width: 80rpx;
      height: 80rpx;
      background-size: 80rpx 80rpx;
      transition: 0.5s;
      transform: scale(1,1)
  }
  .weui-cells_after-title{
    width: 92%;
    margin:0 auto;
    border:1px solid #ddd;
  }
  
  .btnSf{
    width: 92%;
    margin:30rpx auto;
    
  }
  .btnSf button{
      background: #57B6BA;
      color: #fff;
  }
  
  .btnSf button:active{
    background: #5BC2C6;
    color: #fff;
  }
  